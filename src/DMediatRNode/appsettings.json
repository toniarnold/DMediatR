{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DMediatR.CertificateProvider": "Debug",
      "DMediatR.NotificationForwarder": "Debug",
      "DMediatR.Remote": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "DMediatR": {
    "Certificate": {
      "HostName": "localhost",
      "FilenamePrefix": "DMediatR",
      "FilePath": "..\\..\\cert",
      "Password": "1234",
      "ValidDays": "2",
      "RenewBeforeExpirationDays": "1",
      "RenewFirewallEnabled": "False"
    },
    // <messagsize>
    "Grpc": {
      "MaxMessageSize": 11000000
    },
    // </messagsize>
    "Host": {
      "Host": "localhost",
      "Port": 18001,
      "OldPort": 18002
    }
  }
}